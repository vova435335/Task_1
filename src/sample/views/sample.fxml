<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.controllers.Controller">
    <children>
        <Canvas fx:id="canvas" height="350.0" width="600.0"/>
        <FlowPane alignment="CENTER" layoutY="350.0" prefHeight="50.0" prefWidth="600.0">
            <children>
                <Button fx:id="input_btn" mnemonicParsing="false" text="Ввод">
                    <FlowPane.margin>
                        <Insets right="40.0"/>
                    </FlowPane.margin>
                </Button>
                <Button fx:id="decision_btn" mnemonicParsing="false" text="Решение">
                    <FlowPane.margin>
                        <Insets right="40.0"/>
                    </FlowPane.margin>
                </Button>
                <Button fx:id="cycle_btn" mnemonicParsing="false" text="Цикл">
                    <FlowPane.margin>
                        <Insets right="40.0"/>
                    </FlowPane.margin>
                </Button>
                <Button fx:id="output_btn" mnemonicParsing="false" text="Вывод"/>
            </children>
        </FlowPane>
    </children>
</AnchorPane>
